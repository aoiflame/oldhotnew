.view-header{
  height: 0rpx;
  background-color: #fff;
}
.view-list-cell{
  height: 280rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}
.view-cell-time{
  width: 250rpx;
  height: 110rpx;
  
}
.view-time{
  text-align: center;
  height: 100%;
  font-style:italic;
  font-weight:600;
  color: #0099CC;
  padding-top: 50rpx;
  padding-bottom: 50rpx;
}
.view-cell-dot{
  width: 50rpx;
  height: 100%;
  
}
.view-dot{
  width: 12rpx;
  height: 12rpx;
  background-color: #999999;
  border-radius: 5rpx;
  margin-left: 10rpx;
  margin-right: 85rpx;
  margin-top: 70rpx;
  margin-bottom: 70rpx;
}
.view-cell-image{
  width: 400rpx;
  height: 100%;
 
}
.view-header-image{
  width: 500rpx;
  height: 200rpx;
  
  margin-left: 5rpx;
  margin-right: 120rpx;
  margin-top: 25rpx;
  margin-bottom: 25rpx;
  
}
.view-row-image{
  width: 500rpx;
  display: flex;
  flex-direction: column;
  flex-wrap: nowrap;
  justify-content: space-between;
}
.view-line{
  width: 4rpx;
  height: 330rpx;
  position: absolute;
  left: 10rpx;
}
.view-line-top{
  width: 100%;
  height: 170rpx;
  background-color: #99CCFF;

}
.view-line-bottom{
  width: 100%;
  height: 170rpx;
  margin-top: 0rpx;
  background-color: #99CCFF;

}
.k-order{
display: flex;
flex-direction: column;
font-size: 10rpx;
}
.k-default{
  display: flex;
  flex-direction: column;
  font-size: 40rpx;
  color: #000;
}
.nav-hover{
  color: #0099CC;
}